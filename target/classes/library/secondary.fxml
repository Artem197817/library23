<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" prefHeight="440.0" prefWidth="198.0" spacing="20.0" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="library.SecondaryController">
    <children>
        <Label text="Каталог">
         <font>
            <Font size="24.0" />
         </font></Label>
        <Button fx:id="secondaryButton" onAction="#buttonCatalog" text="Каталог">
         <font>
            <Font size="18.0" />
         </font></Button>
      <Button mnemonicParsing="false" onAction="#buttonSearch" text="Найти книгу">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button mnemonicParsing="false" onAction="#buttonIssue" text="Выдать книгу">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button mnemonicParsing="false" text="Вернуть книгу">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button mnemonicParsing="false" text="Новая книга">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button mnemonicParsing="false" text="Удалить книгу">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
